<table class="table table-hover table-inverse">
  <% if @range == "User" %>
    <h2>Users search for "<%="#{@word}"%>"</h2>
    <%= render 'users/index', customers: @customers %>
  <% elsif @range == "Word" %>
    <h2>Posts search for "<%="#{@word}"%>"</h2>
    <%= render 'public/posts/timeline', posts: @posts %>
  <% end %>
</table>