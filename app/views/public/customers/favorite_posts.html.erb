<div class="container">
  <div class="row">
    <%= render 'layouts/errors', obj: @post %>
    <div class=".col-md-3">
      <h1>トップページ</h1>
      <%= render 'layouts/search' %>
    </div>
    <div class=".col-md-6">
      <h1>タイムライン</h1>
      <%= render 'public/posts/index', posts: @posts %>
    </div>
    <div class=".col-md-3">
      <%= render 'public/posts/form', post: @post %>
    </div>
  </div>
</div>